    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/${workspaceFolderBasename}.elf",
            "request": "launch",
            "device": "STM32F407VE",
            "svdFile": "./STM32F407.svd",
            "type": "cortex-debug",
            "servertype": "openocd",
            "interface": "swd",
            "configFiles": [                  
                "${workspaceRoot}/openocd.cfg"
            ],
            "preLaunchTask": "build"
        }
    ]
}